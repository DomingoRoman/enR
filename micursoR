# Datos tomados de vocales repetidas

data <- read.csv("http://domingo-roman.net/velocidad.csv")

feme <- subset(data, sex=="f")
masc <- subset(data, sex =="h")

x_feme <- mean(feme[,2])
x_masc <- mean(masc[,2])
xs <- c(x_masc,x_feme)

boxplot(masc[,2], feme[,2], main ="Velocidad (f/seg) hombres y mujeres", col = "grey95")
par(new=T)
points((xs), col ="blue",type="o", lty=2, pch="X")

hist(feme[,2])
hist(masc[,2])

x_feme
x_masc
